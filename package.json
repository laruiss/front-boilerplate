{
  "name": "front-boilerplate",
  "description": "A ready-to-code set of asset and configuration files to begin coding in HTML5, with jquery, modernizr and sass (compass)",
  "version": "0.3.0",
  "scripts": {
    "start": "npm run build && npm run dev",
    "dev": "browser-sync start --server target --files 'target/**/*.*'",
    "build": "npm run build:html && npm run build:css && npm run build:js",
    "build:html": "mkdir -p target && cp public/index.html target/",
    "build:css": "compass compile",
    "build:js": "webpack",
    "watch": "node watcher.js"
  },
  "author": "Stanislas Ormi√®res",
  "dependencies": {
    "express": "~3.3.4",
    "jquery": "^2.1.4"
  },
  "devDependencies": {
    "autoprefixer": "^6.1.2",
    "babel-core": "^6.3.15",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "browser-sync": "^2.10.0",
    "cssnano": "^3.3.2",
    "modernizr-webpack-plugin": "^1.0.5",
    "normalize.css": "^3.0.3",
    "postcss": "^5.0.12",
    "postcss-color-rgba-fallback": "^2.0.0",
    "postcss-conditionals": "^2.0.0",
    "postcss-cssnext": "^2.2.0",
    "postcss-discard-comments": "^2.0.2",
    "postcss-each": "^0.7.1",
    "postcss-for": "^2.0.1",
    "postcss-import": "^7.1.3",
    "postcss-inuity-layout": "0.0.2",
    "postcss-mixins": "^3.0.1",
    "postcss-nested": "^1.0.0",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  }
}
